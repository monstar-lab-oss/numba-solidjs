{
  "name": "ia-screen-spec",
  "private": true,
  "version": "0.4.1",
  "description": "Figma plugin for screen definition document",
  "scripts": {
    "type": "tsc --noEmit",
    "test": "uvu -r esbuild-register ./src/lib/utils \".*.test.ts\"",
    "lint": "eslint .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:main": "esbuild src/main.ts --outfile=dist/code.js --bundle",
    "build:ui": "vite build",
    "prebuild": "node -e 'fs.rmSync(`dist`, { recursive: true, force: true })'",
    "build": "concurrently -c yellow,blue -P \"npm:build:* -- {*}\" -- --minify",
    "dev": "concurrently -k -c yellow,blue -P \"npm:build:* -- {*}\" -- --watch",
    "test-storybook": "test-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monstar-lab-group/ia-screen-spec.git"
  },
  "author": "Jinma Yamashita",
  "bugs": {
    "url": "https://github.com/monstar-lab-group/ia-screen-spec/issues"
  },
  "homepage": "https://github.com/monstar-lab-group/ia-screen-spec#readme",
  "dependencies": {
    "clsx": "1.2.1",
    "fast-deep-equal": "3.1.3",
    "solid-js": "1.4.8"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@figma/plugin-typings": "1.51.0",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.10",
    "@storybook/builder-vite": "0.2.2",
    "@storybook/html": "6.5.10",
    "@storybook/test-runner": "0.7.1",
    "@storybook/testing-library": "0.0.13",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.36.0",
    "@typescript-eslint/parser": "5.36.0",
    "autoprefixer": "10.4.8",
    "babel-loader": "8.2.5",
    "concurrently": "7.3.0",
    "esbuild": "0.15.1",
    "esbuild-register": "3.3.3",
    "eslint": "8.23.0",
    "postcss": "8.4.16",
    "postcss-import": "14.1.0",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "typescript-plugin-css-modules": "3.4.0",
    "uvu": "0.5.6",
    "vite": "3.0.5",
    "vite-plugin-singlefile": "0.11.1",
    "vite-plugin-solid": "2.3.0"
  }
}
