{
  "name": "ia-screen-spec",
  "private": true,
  "version": "0.2.0",
  "description": "Figma plugin for screen definition document",
  "scripts": {
    "type": "tsc --noEmit",
    "test": "uvu -r esbuild-register src/lib/__tests__",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:main": "esbuild src/main.ts --outfile=dist/code.js --bundle",
    "build:ui": "vite build",
    "prebuild": "node -e 'fs.rmSync(`dist`, { recursive: true, force: true })'",
    "build": "concurrently -c yellow,blue -P \"npm:build:* -- {*}\" -- --minify",
    "dev": "concurrently -k -c yellow,blue -P \"npm:build:* -- {*}\" -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monstar-lab-group/ia-screen-spec.git"
  },
  "author": "Jinma Yamashita",
  "bugs": {
    "url": "https://github.com/monstar-lab-group/ia-screen-spec/issues"
  },
  "homepage": "https://github.com/monstar-lab-group/ia-screen-spec#readme",
  "dependencies": {
    "clsx": "1.2.1",
    "solid-js": "1.4.7"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@figma/plugin-typings": "1.49.0",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-vite": "0.1.39",
    "@storybook/html": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@types/node": "18.0.3",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "concurrently": "7.2.2",
    "esbuild": "0.14.49",
    "esbuild-register": "3.3.3",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.12",
    "tailwindcss": "3.1.5",
    "typescript": "4.7.4",
    "uvu": "0.5.6",
    "vite": "2.9.13",
    "vite-plugin-singlefile": "0.10.0",
    "vite-plugin-solid": "2.2.6"
  }
}
